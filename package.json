{
  "name": "spur",
  "displayName": "Spur VSCode",
  "description": "VSCode support for Spur.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/benaepli/spur-vscode.git"
  },
  "publisher": "baepli",
  "contributes": {
    "languages": [
      {
        "id": "spur",
        "aliases": [
          "spur",
          "spur"
        ],
        "extensions": [
          ".spur"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "spur",
        "scopeName": "source.spur",
        "path": "./syntaxes/spur.tmLanguage.json"
      }
    ]
  }
}